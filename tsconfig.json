{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "#/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "vite-plugin-pwa/solid"
    ],
    "strictNullChecks": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
